{
  "name": "gumga-many-to-one-ng",
  "version": "1.1.2",
  "description": "Gumga Many To One",
  "main": "./src/many-to-one.js",
  "directories": {
    "example": "example"
  },
  "dependencies": {
    "angular": "1.5.8",
    "angular-ui-bootstrap": "2.3.2"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ],
      [
        "uglifyify",
        {
          "mangle": false,
          "compress": {
            "sequences": true,
            "dead_code": true,
            "booleans": true,
            "conditionals": true,
            "if_return": false,
            "drop_console": false,
            "keep_fnames": true
          },
          "output": {
            "comments": false
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "angular-ui-bootstrap": "^2.5.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.13.2",
    "babelify": "^7.3.0",
    "live-server": "^1.1.0",
    "npm-run-all": "^2.3.0",
    "simplifyify": "^3.2.1"
  },
  "scripts": {
    "start": "npm-run-all -p live-server watch",
    "live-server": "live-server --host=localhost --port=9000",
    "watch": "simplifyify 'src/many-to-one.js' -o 'dist/gumga-many-to-one.js' --bundle --minify --watch --debug",
    "prod": "simplifyify 'src/many-to-one.js' -o 'dist/gumga-many-to-one.js' --bundle --minify",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "gumga",
    "many to one",
    "angular 1.x"
  ],
  "author": "Gumga Tecnologia da Informação S/A",
  "license": "LGPL-3.0"
}
